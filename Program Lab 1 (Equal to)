// Program Lab 1.cpp : This file contains the 'main' function. Program execution begins and ends there.
// Create a program that that checks if the two input numbers is equal or the same
//
#include <iostream>

using namespace std;

int main() {

    int a, b;

    cout << "Please Enter you first number: " << endl;
    cin >> a;

    cout << "Please Enter your second number: " << endl;
    cin >> b;

    // check if greater than or less than 
    if (a > b) {
        cout << a << " is greater than " << b << endl;
    }
    else if (a < b) {
        cout << "\n" << a << " is lower than " << b << endl;
    }
    // Check if equal or not
    else if (a == b) {

        cout << "\n" << "First Number and Second Number are Equal";
    }
   
    return 0;
}
